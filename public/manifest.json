{
    "manifest_version": 2,
    "name": "Foobar",
    "version": "1.0",
    "description": "Foobar extension made with React.",
    "icons": {
        "192": "logo192.png",
        "512": "logo512.png"
    },
    "browser_action": {
        "default_icon": "logo192.png",
        "default_title": "Foobar",
        "default_popup": "index.html"
    },
    "devtools_page": "devtools.html",
    "sidebar_action": {
        "default_icon": "logo192.png",
        "default_title": "Foobar",
        "default_panel": "index.html"
    }
}